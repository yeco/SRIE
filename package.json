{
  "name": "srie",
  "version": "0.1.0",
  "engines": {
    "node": "13.9.0",
    "yarn": "1.x"
  },
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "build": "db-migrate up && next build",
    "start": "cross-env NODE_ENV=production node server.js -p $PORT",
    "lint": "./node_modules/.bin/eslint ."
  },
  "dependencies": {
    "cross-env": "^7.0.2",
    "body-parser": "^1.19.0",
    "db-migrate": "^0.11.6",
    "db-migrate-pg": "^1.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-http-context": "^1.2.3",
    "fastest-validator": "^1.4.0",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "massive": "^6.2.0",
    "next": "^9.3.4",
    "next-connect": "^0.6.0",
    "next-i18next": "^4.2.1",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "swr": "^0.1.18"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "node_modules/.bin/eslint"
    ]
  },
  "cacheDirectories": [
    ".next/cache"
  ]
}
